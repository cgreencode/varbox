{
    "name": "varbox/varbox",
    "description": "VarBox",
    "license": "MIT",
    "homepage": "https://varbox.io",
    "keywords": [
        "laravel",
        "varbox"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Andrei Badea",
            "email": "zbiller@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/contracts": "~5.8.0",
        "illuminate/support": "~5.8.0",
        "illuminate/auth": "~5.8.0",
        "illuminate/database": "~5.8.0",
        "illuminate/http": "~5.8.0",
        "illuminate/routing": "~5.8.0",
        "illuminate/view": "~5.8.0",
        "illuminate/validation": "~5.8.0",
        "illuminate/console": "~5.8.0",
        "intervention/image": "^2.5",
        "predis/predis": "^1.1",
        "laravelcollective/html": "~5.8.0",
        "league/flysystem-aws-s3-v3": "^1.0",
        "spatie/laravel-image-optimizer": "^1.4",
        "spatie/laravel-backup": "^6.2",
        "proengsoft/laravel-jsvalidation": "^2.4",
        "davejamesmiller/laravel-breadcrumbs": "^5.2",
        "kalnoy/nestedset": "^4.3",
        "pbmedia/laravel-ffmpeg": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5|^8.0",
        "orchestra/testbench": "~3.8.0",
        "orchestra/testbench-dusk": "~3.8.0",
        "doctrine/dbal": "^2.9",
        "predis/predis": "^1.1",
        "roquie/laravel-dusk-select2": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Varbox\\": "src/",
            "Varbox\\Seed\\": "database/seeds/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Varbox\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Varbox\\VarboxServiceProvider"
            ],
            "aliases": {
                "Varbox": "Varbox\\Facades\\VarboxFacade"
            }
        }
    }
}
